package com.youweather.sgg;


/**
 * @author sim1
 * La classe contiene gli elementi che andranno poi a costituire lo stream dei post 
 * pubblicati dagli utenti, con adeguati metodi getter e setter
 *
 */


public class ItemDetails {
	
	private String citta ;
	private String condizioni;
	private String utente;
	private String temperatura;
	private int imageNumber;
	
	public String getCity() {
		return citta;
	}
	public void setCity(String citta) {
		this.citta = citta;
	}
	public String getConditions() {
		return condizioni;
	}
	public void setConditions(String condizioni) {
		this.condizioni = condizioni;
	}
	public String getUser() {
		return utente;
	}
	public void setUser(String utente) {
		this.utente = utente;
	}
	public int getImageNumber() {
		return imageNumber;
	}
	public void setImageNumber(int imageNumber) {
		this.imageNumber = imageNumber;
	}
	public String getTemperature() {
		return temperatura;
	}
	public void setTemperature(String temperatura) {
		this.temperatura = temperatura;
	}

	
}
